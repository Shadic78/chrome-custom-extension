{
  "name": "Ads bypass for some pages",
  "description": "Allows to bypass the 30 seconds ads to get download links on CompucaliTV and the ads on japan-paw",
  "version": "1.3.0",
  "author": "Shadic78 & HikingCarrot7",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "webNavigation", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/icon16.png",
      "32": "/images/icon32.png",
      "48": "/images/icon48.png",
      "128": "/images/icon128.png"
    }
  },
  "icons": {
    "16": "/images/icon16.png",
    "32": "/images/icon32.png",
    "48": "/images/icon48.png",
    "128": "/images/icon128.png"
  },
  "host_permissions": ["https://*/*"],
  "options_page": "/options/options.html"
}
